{
  "name": "vue-grecaptcha",
  "version": "1.0.1",
  "description": "Google Recaptcha for VueJS",
  "main": "dist/vue-grecaptcha.js",
  "unpkg": "dist/vue-grecaptcha.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rm -fr dist && webpack && uglifyjs --compress --mangle --output dist/vue-grecaptcha.min.js -- dist/vue-grecaptcha.js",
